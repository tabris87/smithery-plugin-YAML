{
    "name": "smithery-plugin-yaml",
    "version": "0.1.3",
    "author": "Adrian Marten",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/tabris87/smithery-plugin-yaml.git"
    },
    "license": "Apache-2.0",
    "description": "A plugin for smithery to support feature oriented programming with YAML",
    "scripts": {
        "test": "jest --runInBand"
    },
    "main": "index.js",
    "bugs": {
        "url": "https://github.com/tabris87/smithery-plugin-yaml/issues"
    },
    "homepage": "https://github.com/tabris87/smithery-plugin-yaml#readme",
    "dependencies": {
        "js-yaml": "^3.14.0",
        "smithery-plugin-json": "^0.1.0",
        "yaml": "^1.10.0"
    },
    "devDependencies": {
        "jest": "^26.0.1",
        "jest-html-reporters": "^1.2.1",
        "smithery": "^0.2.0"
    },
    "jest": {
        "testEnvironment": "node",
        "roots": [
            "test"
        ],
        "collectCoverage": true,
        "coverageReporters": [
            "text",
            "html"
        ],
        "reporters": [
            "default",
            [
                "jest-html-reporters",
                {
                    "filename": "./test/latestTests.html",
                    "expand": false,
                    "pageTitle": "smithery-plugin-yaml",
                    "hideIcon": true
                }
            ]
        ]
    }
}
